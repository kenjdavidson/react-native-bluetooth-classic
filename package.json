{
  "name": "react-native-bluetooth-classic",
  "version": "1.70.0",
  "description": "Implement bluetooth classic (serial) on Android (Bluetooth Adapter) and IOS (External Accessory)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rm -rf lib",
    "clean-win": "rd /s /q lib",
    "tsc": "tsc",
    "tsc-win": "npx tsc",
